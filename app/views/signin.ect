<% extend 'layout' %>
<form action="/users/signin" method="post">
	<div>
  	<label>Email:</label>
  	<input type="text" name="email"/>
	</div>
	<div>
  	<label>Password:</label>
  	<input type="password" name="password"/>
	</div>
	<div>
		<input id="token" type="hidden" name="_csrf" value="<%= @csrf %>">
	</div>
	<div>
	  <input type="submit" value="Sign in"/>
	</div>
</form>
